{
  "name": "rbtech",
  "version": "1.0.0",
  "description": "SAAS dashboard",
  "main": "index.js",
  "scripts": {
    "start": "node build/src/server.js",
    "build": "tsc --p tsconfig.build.json && npm run copy-files",
    "test": "jest --runInBand",
    "dev": "npm run migrate && nodemon src/server.ts 'src/**/*.ts' --exec \"ts-node\" --project tsconfig.build.json src/server.ts ",
    "migrate": "npx sequelize-cli db:migrate",
    "createmigration": "npx sequelize-cli migration:create --name",
    "copy-files": "cpy \"src/utils/emailTemplates/**/*.*\" \"build/src/utils/emailTemplates\""
  },
  "author": "Moshood Alimi",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.19.8",
    "cpy-cli": "^4.2.0",
    "jest": "^29.3.1",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "africastalking": "^0.6.2",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "google-auth-library": "^8.7.0",
    "googleapis": "^111.0.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "nocache": "^3.0.4",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.22",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "unique-username-generator": "^1.1.3",
    "uuidv4": "^6.2.13"
  }
}
